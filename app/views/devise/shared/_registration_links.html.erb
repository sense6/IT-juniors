<%- if controller_name != 'sessions' %>
   <%= link_to "Log in", new_session_path(resource_name), class: 'btn btn-success' %>
 <% end -%>

 <%- if devise_mapping.omniauthable? %>
   <%- resource_class.omniauth_providers.each do |provider| %>
     <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class: 'btn btn-success' %>
   <% end -%>
 <% end -%>
